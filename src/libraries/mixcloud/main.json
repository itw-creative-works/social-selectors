
{
  "global": {
    // Indicators
    "isOnPage": {
      "url": "/mixcloud\\.com/",
    },
    "isOnMainSite": {
      "selector": "",
    },
    "isLoggedOut": {
      "selector": "",
      "object": { "selector": "a button", "filters": [ {"match": { "innerText": "/Join/i" }} ] },
    },
    "signUpButton": {
      "selector": "",
